{
    "manifest_version": 2,

    "name": "Lending Club Folio",
    "description": "This extension extracts detailed notes information automatically.",
    "version": "1.0",
	"background": {
		"page": "background.html"
	},

    "page_action": {
	"default_icon": "icon.png",
	"default_popup" : "popup.html"
	},
    
    "permissions": [
	"tabs"
	],

	"icons" : {
		"48": "icon.png"
	}
}
