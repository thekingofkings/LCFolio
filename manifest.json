{
    "manifest_version": 2,

    "name": "Lending Club Folio",
    "description": "This extension extracts detailed notes information automatically.",
    "version": "1.0",
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	},

    "page_action": {
	"default_icon": "icon.png"
//	"default_popup" : "popup.html"
	},
    
    "permissions": [
	"tabs",
	"activeTab"
	],

	"icons" : {
		"48": "icon.png"
	}
}
